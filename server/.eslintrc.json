{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["google"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "object-curly-spacing": ["error", "always"],
        "linebreak-style": 0,
        "no-undef": 2
    }
}
